{
  "name": "mydiary",
  "version": "1.0.0",
  "description": "MyDiary is an online journal where users can pen down their thoughts and feelings.",
  "main": "app.js",
  "scripts": {
    "test": "npm run build && nyc --reporter=html --reporter=lcov --reporter=text mocha --timeout 250000 server/test/*.js --compilers js:babel-core/register --exit",
    "build": "babel server -d ./dist",
    "lint": "./node_modules/.bin/eslint server/app.js",
    "start": "npm run build && nodemon --watch server --exec babel-node server/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sulenchy/MyDiary.git"
  },
  "keywords": [
    "Node",
    "Express",
    "Javascript",
    "Mocha"
  ],
  "author": "sulenchy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sulenchy/MyDiary/issues"
  },
  "homepage": "https://github.com/sulenchy/MyDiary#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai-http": "^4.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "istanbul": "^0.4.5",
    "morgan": "^1.9.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^12.0.2"
  }
}
